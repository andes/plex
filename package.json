{
    "name": "@andes/plex",
    "version": "8.0.5",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andes/plex.git"
    },
    "author": "Red TICs",
    "license": "GPL-3.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --configuration production --base-href '/plex/app/'",
        "build:test": "ng build --configuration production",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "serve": "ng serve --progress=false",
        "prepublishOnly": "rm -rf dist && tsc -p ./ --outDir dist/",
        "test:ci": "ng test --watch=false --progress=false --browsers=ChromeHeadlessCI",
        "cypress": "wait-on http://localhost:4200 && cypress run",
        "test:cypress": "run-p --race serve cypress",
        "test:cypress:local": "APPLITOOLS_IS_DISABLED=true run-p --race serve cypress",
        "cypress:open": "APPLITOOLS_IS_DISABLED=true cypress open",
        "cypress:open:mobile": "APPLITOOLS_IS_DISABLED=true cypress open --config-file cypress-mobile.json",
        "commitlint-circle": "commitlint-circle",
        "web-server": "http-server dist/app -p 4200 -s --proxy http://localhost:4200?",
        "build:doc": "npx compodoc -p src/lib/tsconfig.json  -d dist/doc/",
        "publish:doc": "npx gh-pages -d dist"
    },
    "main": "./dist/index.js",
    "dependencies": {
        "@andes/icons": "^1.1.12",
        "@angular/material": "^12.2.13",
        "@mdi/font": "^5.0.45",
        "@types/estree": "^1.0.2",
        "@types/intro.js": "^2.4.6",
        "@types/jquery": "^3.3.38",
        "@types/quill": "^1.3.10",
        "@types/tether-shepherd": "^1.2.28",
        "animate.css": "^3.7.0",
        "awesome-phonenumber": "^1.2.0",
        "bootstrap": "4.0.0-alpha.6",
        "chart.js": "^2.6.0",
        "core-js": "^2.4.1",
        "hammerjs": "^2.0.8",
        "http-server": "^0.12.3",
        "intro.js": "^2.9.3",
        "jquery": "^3.3.1",
        "loaders.css": "^0.1.2",
        "moment": "^2.16.0",
        "ng2-charts": "^2.4.2",
        "ngx-infinite-scroll": "^8.0.1",
        "ngx-quill": "^4.0.0",
        "node-waves": "^0.7.5",
        "quill": "^1.3.6",
        "rxjs": "^7.0.0",
        "selectize": "0.12.4",
        "sweetalert2": "^6.6.2",
        "tether-shepherd": "^1.8.1",
        "tslib": "^2.0.0",
        "url-parse": "^1.1.9",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.2.18",
        "@angular/animations": "^12.2.17",
        "@angular/cdk": "^12.2.13",
        "@angular/cli": "^12.2.18",
        "@angular/common": "^12.2.17",
        "@angular/compiler": "^12.2.17",
        "@angular/compiler-cli": "^12.2.17",
        "@angular/core": "^12.2.17",
        "@angular/forms": "^12.2.17",
        "@angular/language-service": "^12.2.17",
        "@angular/platform-browser": "^12.2.17",
        "@angular/platform-browser-dynamic": "^12.2.17",
        "@angular/platform-server": "^12.2.17",
        "@angular/router": "^12.2.17",
        "@applitools/eyes-cypress": "^3.8.21",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-angular": "^8.3.4",
        "@semantic-release/changelog": "^3.0.6",
        "@semantic-release/git": "^9.0.0",
        "@types/cypress-axe": "^0.8.0",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^12.11.1",
        "axe-core": "^4.1.4",
        "codelyzer": "^6.0.0",
        "commitlint-circle": "^1.0.0",
        "cypress": "^4.12.1",
        "cypress-axe": "^0.12.2",
        "gh-pages": "^2.2.0",
        "husky": "^4.2.3",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "^6.4.3",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "^2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "npm-run-all": "^4.1.5",
        "semantic-release": "^17.0.4",
        "ts-node": "~3.0.4",
        "tslint": "^6.1.3",
        "typescript": "~4.3.2",
        "wait-on": "^3.3.0"
    },
    "release": {
        "branches": [
            {
                "name": "master"
            },
            {
                "name": "beta",
                "prerelease": true
            }
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/npm",
            "@semantic-release/git",
            "@semantic-release/github"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-angular"
        ],
        "rules": {
            "type-enum": [
                2,
                "always",
                [
                    "docs",
                    "feat",
                    "fix",
                    "perf",
                    "refactor",
                    "revert",
                    "style",
                    "test",
                    "chore"
                ]
            ]
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
