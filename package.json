{
    "name": "@andes/plex",
    "version": "6.15.3",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andes/plex.git"
    },
    "author": "Red TICs",
    "license": "GPL-3.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod --base-href '/plex/'",
        "build:test": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "serve": "ng serve --progress=false",
        "prepublishOnly": "rm -rf dist && tsc -p ./ --outDir dist/",
        "test:ci": "ng test --watch=false --progress=false --browsers=ChromeHeadlessCI",
        "cypress": "wait-on http://localhost:4200 && cypress run",
        "test:cypress": "run-p --race serve cypress",
        "test:cypress:local": "APPLITOOLS_IS_DISABLED=true run-p --race serve cypress",
        "cypress:open": "APPLITOOLS_IS_DISABLED=true cypress open",
        "commitlint-circle": "commitlint-circle",
        "web-server": "http-server dist -p 4200 -s --proxy http://localhost:4200?"
    },
    "main": "./dist/index.js",
    "dependencies": {
        "@andes/icons": "^1.1.2",
        "@mdi/font": "^5.0.45",
        "@types/intro.js": "^2.4.6",
        "@types/jquery": "^3.3.6",
        "@types/tether-shepherd": "^1.2.28",
        "animate.css": "^3.7.0",
        "awesome-phonenumber": "^1.2.0",
        "bootstrap": "4.0.0-alpha.6",
        "chart.js": "^2.6.0",
        "core-js": "^2.4.1",
        "hammerjs": "^2.0.8",
        "http-server": "^0.12.3",
        "intro.js": "^2.9.3",
        "jquery": "^3.3.1",
        "loaders.css": "^0.1.2",
        "moment": "^2.16.0",
        "ng2-charts": "^1.6.0",
        "ngx-infinite-scroll": "^8.0.1",
        "ngx-quill": "^4.0.0",
        "node-waves": "^0.7.5",
        "quill": "^1.3.6",
        "selectize": "0.12.4",
        "sweetalert2": "^6.6.2",
        "tether-shepherd": "^1.8.1",
        "url-parse": "^1.1.9",
        "zone.js": "^0.9.1"
    },
    "peerDependencies": {
        "@angular/cdk": ">=5.0.0",
        "@angular/http": ">=5.0.0",
        "@angular/material": ">=5.0.0",
        "rxjs": "6.5.2",
        "rxjs-compat": ">=5.0.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.801.3",
        "@angular/animations": "^8.1.0",
        "@angular/cdk": "^7.0.1",
        "@angular/cli": "^8.1.0",
        "@angular/common": "^8.1.0",
        "@angular/compiler": "^8.1.0",
        "@angular/compiler-cli": "^8.1.0",
        "@angular/core": "^8.1.0",
        "@angular/forms": "^8.1.0",
        "@angular/http": "^7.0.0",
        "@angular/language-service": "^8.1.0",
        "@angular/material": "^7.0.1",
        "@angular/platform-browser": "^8.1.0",
        "@angular/platform-browser-dynamic": "^8.1.0",
        "@angular/platform-server": "^8.1.0",
        "@angular/router": "^8.1.0",
        "@applitools/eyes-cypress": "^3.8.21",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-angular": "^8.3.4",
        "@semantic-release/changelog": "^3.0.6",
        "@semantic-release/git": "^9.0.0",
        "@types/jasmine": "^2.5.45",
        "@types/node": "^6.0.117",
        "codelyzer": "^4.4.4",
        "commitlint-circle": "^1.0.0",
        "cypress": "^4.6.0",
        "gh-pages": "^2.2.0",
        "husky": "^4.2.3",
        "jasmine-core": "^3.5.0",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "^4.4.1",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage-istanbul-reporter": "^2.1.0",
        "karma-jasmine": "^2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "npm-run-all": "^4.1.5",
        "rxjs-compat": "^6.2.2",
        "semantic-release": "^17.0.4",
        "ts-node": "~3.0.4",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.4.5",
        "wait-on": "^3.3.0"
    },
    "release": {
        "branches": [
            {
                "name": "master"
            },
            {
                "name": "beta",
                "prerelease": true
            }
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/npm",
            "@semantic-release/git",
            "@semantic-release/github"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-angular"
        ],
        "rules": {
            "type-enum": [
                2,
                "always",
                [
                    "docs",
                    "feat",
                    "fix",
                    "perf",
                    "refactor",
                    "revert",
                    "style",
                    "test",
                    "chore"
                ]
            ]
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
