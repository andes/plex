{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "plex-demo": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/plex-demo",
            "sourceRoot": "projects/plex-demo/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/plex-demo",
                        "index": "projects/plex-demo/src/index.html",
                        "main": "projects/plex-demo/src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "projects/plex-demo/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/plex-demo/src/favicon.ico",
                            "projects/plex-demo/src/assets"
                        ],
                        "styles": [
                            "projects/plex-demo/src/styles.scss"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "dist",
                                "dist/styles",
                                "dist/css"
                            ]
                        },
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "3mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "plex-demo:build:production"
                        },
                        "development": {
                            "browserTarget": "plex-demo:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "plex-demo:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "projects/plex-demo/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/plex-demo/src/favicon.ico",
                            "projects/plex-demo/src/assets"
                        ],
                        "styles": [
                            "projects/plex-demo/src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        }
    },
    "cli": {
        "warnings": {
            "typescriptMismatch": false
        },
        "analytics": "8308d0fc-1e86-41d4-86b1-f56379624dcd",
        "schematicCollections": [
            "angular-eslint"
        ]
    },
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "app",
            "style": "css"
        },
        "@schematics/angular:directive": {
            "prefix": "app"
        }
    }
}